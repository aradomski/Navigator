<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pl.edu.radomski.navigator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".examples.SimpleActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".examples.GroupedResultsForResultActivity" />
        <activity android:name=".examples.ForResultActivity" />
        <activity android:name=".examples.GroupedParamsActivity" />
        <activity android:name=".examples.ParamsActivity" />
        <activity android:name=".examples.ParamsForResultActivity" />
        <activity android:name=".examples.GroupedParamsForResultActivity" />
        <activity android:name=".examples.AllParamsActivity" />
        <activity android:name=".examples.GroupedActivity" />
        <activity android:name=".examples.ActivityWithResultOption" />
        <activity android:name=".examples.SimpleNamedMethodActivity" />
        <activity android:name=".examples.NamedParamsAndResultActivity" />
        <activity android:name=".examples.databinding.DataBindingTestActivity" />
    </application>

</manifest>
